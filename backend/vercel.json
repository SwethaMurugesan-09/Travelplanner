{
    "version": 2,
    "builds": [
        { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/server.js" },
        { "src": "/signup/(.*)", "dest": "/server.js" },
        { "src": "/package/(.*)", "dest": "/server.js" },
        { "src": "/(.*)", "dest": "/server.js" }
    ]
}
